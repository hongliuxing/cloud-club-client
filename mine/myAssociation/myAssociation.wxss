page{
  background: #f9f9f9;
  display: flex;
  flex:1;
  flex-direction: column;
  height:100%;
}
.principal{
  margin-right: 40rpx;
}
.noMore{
  display: flex;
  flex:1;
  align-items: center;
  flex-direction: column;
  font-size: 26rpx;
  color:#676767;
  margin-top: 35%;
}
.noMoreimage{
  width: 260rpx;
  height: 230rpx;
}
.noMore >view{
  margin-top: 10rpx;
}
.noMore >text{
  margin-top: 40rpx;
}
.noMore >view>text:nth-child(1){
  color:#007eb9;
  margin-right: 5rpx;
}
.responsible{
  margin-top: 50rpx;
}
.responsibleTitle{
  display: inline-block;
  font-size: 26rpx;
  color:#999;
  margin-left:30rpx;
  margin-bottom: 20rpx;
}
.responsible-name{
  font-weight: 800 !important;
  color:#000 !important;
}
.responsible-value{
  color:#7c9bd6 !important;
}
.responsible-value::after{
  content: "äºº";
  color:#999;
}
